#
# Name : alaises_vbox
# Desc : Virtual Box's command line utility alaises
#

function vbox-list() { VBoxManage list vms; }
function vbox-start() { VBoxManage startvm $1 --type headless; }
function vbox-pause() { VBoxManage controlvm $1 pause; }
function vbox-resume() { VBoxManage controlvm $1 resume; }
function vbox-reset() { VBoxManage controlvm $1 reset; }
function vbox-poff() { VBoxManage controlvm $1 poweroff; }
function vbox-sdown() { VBoxManage controlvm $1 acpipowerbutton; }
function vbox-save() { VBoxManage controlvm $1 savestate; }
